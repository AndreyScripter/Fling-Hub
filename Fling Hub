local ScreenGui = Instance.new("ScreenGui") local MainFrame = Instance.new("Frame") local Title = Instance.new("TextLabel") local FlingButton = Instance.new("TextButton") local ExitButton = Instance.new("TextButton") local IconButton = Instance.new("TextButton")

-- Proprietăți UI ScreenGui.Parent = game.CoreGui

MainFrame.Size = UDim2.new(0, 300, 0, 150) MainFrame.Position = UDim2.new(0.5, -150, 0.4, -75) MainFrame.BackgroundColor3 = Color3.fromRGB(50, 50, 50) MainFrame.BorderSizePixel = 2 MainFrame.BorderColor3 = Color3.fromRGB(0, 0, 0) MainFrame.Active = true MainFrame.Draggable = true MainFrame.Parent = ScreenGui

Title.Size = UDim2.new(1, 0, 0.2, 0) Title.BackgroundTransparency = 1 Title.Text = "Fling Hub" Title.Font = Enum.Font.SourceSansBold Title.TextSize = 20 Title.TextColor3 = Color3.fromRGB(255, 140, 0) Title.Parent = MainFrame

FlingButton.Size = UDim2.new(0.6, 0, 0.3, 0) FlingButton.Position = UDim2.new(0.2, 0, 0.4, 0) FlingButton.BackgroundColor3 = Color3.fromRGB(255, 140, 0) FlingButton.Text = "Fling All" FlingButton.Font = Enum.Font.SourceSansBold FlingButton.TextSize = 18 FlingButton.Parent = MainFrame

ExitButton.Size = UDim2.new(0.15, 0, 0.2, 0) ExitButton.Position = UDim2.new(0.85, 0, 0, 0) ExitButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0) ExitButton.Text = "X" ExitButton.Font = Enum.Font.SourceSansBold ExitButton.TextSize = 18 ExitButton.TextColor3 = Color3.fromRGB(255, 255, 255) ExitButton.Parent = MainFrame

IconButton.Size = UDim2.new(0, 50, 0, 50) IconButton.Position = UDim2.new(0.05, 0, 0.05, 0) IconButton.BackgroundColor3 = Color3.fromRGB(255, 140, 0) IconButton.Text = "FH" IconButton.Font = Enum.Font.SourceSansItalic IconButton.TextSize = 18 IconButton.TextColor3 = Color3.fromRGB(0, 0, 0) IconButton.Visible = false IconButton.Active = true IconButton.Draggable = true IconButton.Parent = ScreenGui

ExitButton.MouseButton1Click:Connect(function() MainFrame.Visible = false IconButton.Visible = true end)

IconButton.MouseButton1Click:Connect(function() MainFrame.Visible = true IconButton.Visible = false end)

FlingButton.MouseButton1Click:Connect(function() -- Scriptul de Fling All activat doar când se apasă butonul loadstring([[PASTEAZĂ AICI SCRIPTUL PE CARE MI L-AI TRIMIS]])() end)


